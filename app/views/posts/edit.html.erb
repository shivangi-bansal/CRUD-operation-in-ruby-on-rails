<h1>EDIT POST</h1>

<%= form_for :post, url: post_path(@post), method: :patch do |f| %>
    <p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %> 
    </p>
    <p>
    <%= f.label :description %> <br>
    <%= f.text_area :description %>
    </p>
    <p>
    <%= f.submit %>
    </p>
    <% end %> 

